function HtmlSelecionaCliente(){var n=new StringBuilder;return n.append("<table cellpadding='0' cellspacing='0' style='FONT-SIZE:10px;FONT-FAMILY:tahoma' border=0>"),n.append("<tr>"),n.append("<td valign='top'>"),n.append("<table cellpadding='0' cellspacing='0' width='100%' bgcolor='#CCCCCC'>"),n.append("<tr>"),n.append("<td colspan='2'><img src='images/login_top.gif' border='0'><\/td>"),n.append("<\/tr>"),n.append("<\/table>"),n.append("<\/td>"),n.append("<\/tr>"),n.append("<tr>"),n.append("<td bgcolor='#CCCCCC'>"),n.append("<table>"),n.append("<tr>"),n.append("<td height='10px'><\/td>"),n.append("<\/tr>"),n.append("<\/table>"),n.append("<table width='100%' border=0 height='100%' style='FONT-SIZE:10px;FONT-FAMILY:tahoma'>"),n.append("<tr>"),n.append("<td align='right' width='75px'>"),n.append("Cliente:"),n.append("<\/td>"),n.append("<td>"),n.append("<select id='cmbSelecionaCliente' onkeypress='SelecionaClienteKeyPress(event);' style='width:200px'><\/select>"),n.append("<\/td>"),n.append("<\/tr>"),n.append("<tr>"),n.append("<td colspan='2' align='center' valign='center' height='36px'>"),n.append("<input type=button class='classbtn' id='btnLogaCliente' onclick='javascript:SelecionaClienteAtivo();' style='height: 20px; width:90px' value=' Ok ' />"),n.append("<\/td>"),n.append("<\/tr>"),n.append("<\/table>"),n.append("<\/td>"),n.append("<\/tr>"),n.append("<\/table>"),n.toString()}function SelecionaClienteKeyPress(n){var t=0;window.event?t=n.keyCode:n.which&&(t=n.which);t==13&&SelecionaClienteAtivo()}function AbrirSelecionaClientes(){var t=verificaSessao(WCAjax.GetDtClientes()),i,n;if(win.create("winSelecionaCliente","SelecionaCliente",173,323,!1,!0,!1),win.insertHtml("winSelecionaCliente",HtmlSelecionaCliente),win.open("winSelecionaCliente",!0),i=$("cmbSelecionaCliente"),i.options.length=0,t!=null)for(n=0;n<t.rows.length;n++)i.options[n]=new Option(t.rows[n].Nome,t.rows[n].Codigo);i.focus()}function SelecionaClienteAtivo(){var n=verificaSessao(WCAjax.SelecionaCliente(GetCampo("cmbSelecionaCliente")));n?location.href="Admin.aspx":(alert("Não foi possível selecionar o cliente"),location.href="Login.aspx")}